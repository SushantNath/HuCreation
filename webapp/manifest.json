{
	"_version": "1.7.0",
	"sap.app": {
		"id": "inbound",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": []
		},
		"dataSources": {
			"mainService": {
				"uri": "/DE4/sap/opu/odata/iwbep/RMTSAMPLEFLIGHT_2/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation0"
					]
				}
			},
			"annotation0": {
				"type": "ODataAnnotation",
				"uri": "annotations/annotation0.xml",
				"settings": {
					"localUri": "annotations/annotation0.xml"
				}
			}
		},
		"offline": false,
		"sourceTemplate": {
			"id": "ui5template.smartTemplate",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"resources": {
			"js": [],
			"css": []
		},
		"dependencies": {
			"minUI5Version": "1.32.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.suite.ui.generic.template": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {}
			},
			"components": {}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ListReport|CarrierCollection": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ListReport/CarrierCollection/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ObjectPage|CarrierCollection": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ObjectPage/CarrierCollection/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ObjectPage|FlightCollection": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ObjectPage/FlightCollection/i18n.properties"
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"SmartFilterBarControlConfigurationExtension|CarrierCollection": {
							"className": "sap.ui.core.Fragment",
							"fragmentName": "inbound.ext.fragment.Customfilter",
							"type": "XML"
						}
					}
				},
				"sap.ui.controllerExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"controllerName": "inbound.ext.controller.ListReportExt",
						"sap.ui.generic.app": {
							"CarrierCollection": {
								"EntitySet": "CarrierCollection",
								"Actions": {
									"ActionCarrierCollection1": {
										"id": "ActionCarrierCollection1button",
										"text": "{@i18n>ActionCarrierCollection1}",
										"press": "onClickActionCarrierCollection1"
									}
								}
							}
						}
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ui.generic.app": {
		"_version": "1.3.0",
		"pages": {
			"ListReport|CarrierCollection": {
				"entitySet": "CarrierCollection",
				"component": {
					"name": "sap.suite.ui.generic.template.ListReport",
					"list": true,
					"settings": {
					
						"multiSelect": true,
						"tableSettings": {
                        "createWithParameterDialog" : {
                            "fields" : {
                                "BusinessPartnerID":{"path": "BusinessPartnerID"},
                                "CurrencyCode" : {"path":"CurrencyCode"},
                                "BillingStatus" : {"path":"BillingStatus"},
                                "DeliveryStatus" : {"path":"DeliveryStatus"},
                                "OpportunityID" : {"path":"OpportunityID"}
                            }
                        }
                    },

						"dataLoadSettings": {
							"loadDataOnAppLaunch": "always"
						}

					}
				},
				"pages": {
					"ObjectPage|CarrierCollection": {
						"entitySet": "CarrierCollection",
						"component": {
							"name": "sap.suite.ui.generic.template.ObjectPage"
						},
						"pages": {
							"ObjectPage|CarrierFlights": {
								"navigationProperty": "CarrierFlights",
								"entitySet": "FlightCollection",
								"component": {
									"name": "sap.suite.ui.generic.template.ObjectPage"
								}
							}
						}
					}
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": ""
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zinboundhu/webapp",
		"_version": "1.1.0"
	}
}