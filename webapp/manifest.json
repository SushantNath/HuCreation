{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.sap.huCreationinboundHu",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"inboundDatasource": {
				"uri": "/sap/opu/odata/sap/ZOTC_DELIVERY_COGS_REVNU_REC_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"useBatch": true,
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "None"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.sap.huCreationinboundHu.view.homeView",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.huCreationinboundHu.i18n.i18n"
				}
			},
			"inboundModel": {
				"dataSource": "inboundDatasource"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
			"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sap.huCreationinboundHu.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
			{
					"name": "RouteSettings",
					"pattern": "RouteSettings",
					"target": [
						"TargetSettings"
					]
				},
				{
					"name": "homePattern",
					"pattern": "homePattern",
					"target": [
						"homeTarget"
					]
				},
					{
					"name": "huCreationPattern",
					"pattern": "huCreationPattern",
					"target": [
						"huCreationTarget"
					]
				}
				
			],
			"targets": {
				"TargetSettings": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "homeView"
				},
					"huCreationTarget": {
					"viewType": "XML",
					"viewName": "huCreation"
				},
				"homeTarget": {
					"viewType": "XML",
					"viewName": "homeView"
				}
			
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zhucreation/webapp",
		"_version": "1.1.0"
	}
}